<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="638.0" prefWidth="1023.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <MenuBar fx:id="menuBar" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <left>
      <TreeView fx:id="effortTree" prefHeight="375.0" prefWidth="311.0" BorderPane.alignment="TOP_LEFT" />
   </left>

   <bottom>
      <ButtonBar fx:id="buttonBar" prefHeight="40.0" prefWidth="200.0" styleClass="dev-tools-button-bar" BorderPane.alignment="CENTER">
        <buttons>
          <Button fx:id="saveButton" mnemonicParsing="false" text="Save" />
            <Button fx:id="editButton" mnemonicParsing="false" text="Edit" />
            <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" />
        </buttons>
      </ButtonBar>
   </bottom>
</BorderPane>
